<div>
  <h3>
    練習内容を編集する
  </h3>
  <%= form_with model: @log, local: true do |f| %>
    <%= f.label "練習時間" %>
    <br>
    <%= f.label "サーブ" %>
    <%= f.text_area :serve, value: '0' %>
    <%= f.label "スマッシュ" %>
    <%= f.text_area :smash, value: '0' %>
    <%= f.label "ボレー" %>
    <%= f.text_area :volley, value: '0' %>
    <%= f.label "ストローク" %>
    <%= f.text_area :stroke, value: '0' %>
    <%= f.label "ゲーム形式" %>
    <%= f.text_area :game, value: '0' %>
    <br>
    <%= f.label "テニスノート" %>
    <br>
    <%= f.text_area :text, rows: "10", cols: "50" %>
    <br>
    <%= f.file_field :image %>
    <%= f.submit "記録" %>
  <% end %>
</div>