<div class="row">
  <aside class="span4">
    <section>
      <%= render 'users/stats' %>
    <h2><%= @title %></h2>
      <% if @users.any? %>
          <% @users.each do |user| %>
            <%= link_to user.name ,user_path(user) %>
          <% end %>
      <% end %>
    </section>
  </aside>
</div>